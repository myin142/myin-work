{
  "version": 1,
  "projects": {
    "work-tracker": {
      "root": "apps/work-tracker",
      "sourceRoot": "apps/work-tracker/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "outputPath": "dist/apps/work-tracker",
            "index": "apps/work-tracker/src/index.html",
            "main": "apps/work-tracker/src/main.tsx",
            "polyfills": "apps/work-tracker/src/polyfills.ts",
            "tsConfig": "apps/work-tracker/tsconfig.app.json",
            "assets": [
              "apps/work-tracker/src/favicon.ico",
              "apps/work-tracker/src/assets",
              "apps/work-tracker/src/404.html"
            ],
            "styles": ["apps/work-tracker/src/styles.scss"],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "baseHref": "/myin-work/",
              "fileReplacements": [
                {
                  "replace": "apps/work-tracker/src/environments/environment.ts",
                  "with": "apps/work-tracker/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "work-tracker:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "work-tracker:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": [
              "apps/work-tracker/tsconfig.app.json",
              "apps/work-tracker/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/work-tracker/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/work-tracker/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "work-tracker-e2e": {
      "root": "apps/work-tracker-e2e",
      "sourceRoot": "apps/work-tracker-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/work-tracker-e2e/cypress.json",
            "tsConfig": "apps/work-tracker-e2e/tsconfig.e2e.json",
            "devServerTarget": "work-tracker:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "work-tracker:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": ["apps/work-tracker-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/work-tracker-e2e/**/*"]
          }
        }
      }
    },
    "cloud": {
      "projectType": "application",
      "root": "apps/cloud",
      "sourceRoot": "apps/cloud/src",
      "architect": {
        "deploy": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "cwd": "apps/cloud",
            "commands": [
              {
                "command": "if [ \"{args.stack}\" = \"undefined\" ]; then cdk deploy --require-approval=never; else cdk deploy --require-approval=never {args.stack}; fi"
              }
            ]
          }
        },
        "destroy": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "cwd": "apps/cloud",
            "commands": [
              {
                "command": "if [ \"{args.stack}\" = \"undefined\" ]; then cdk destroy -f; else cdk destroy -f {args.stack}; fi"
              }
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/cloud/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "daily-time": {
      "root": "libs/daily-time",
      "sourceRoot": "libs/daily-time/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": [
              "libs/daily-time/tsconfig.lib.json",
              "libs/daily-time/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/daily-time/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/daily-time/jest.config.js",
            "passWithNoTests": true
          }
        },
        "package": {
          "builder": "@myin/aws-cdk:package",
          "options": {
            "entryFile": "libs/daily-time/src/index.ts",
            "tsConfig": "libs/daily-time/tsconfig.lib.json",
            "outputPath": "dist/libs/daily-time"
          }
        }
      }
    },
    "cloud-shared": {
      "root": "libs/cloud-shared",
